<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="reservation.vaccine.mapper.UserMapper">

    <select id="findAll" resultType="reservation.vaccine.domain.UserInfo">
        select * from user_info
    </select>

    <select id="findUserById" resultType="int" parameterType="String">
        select Uid from user_info where ID = #{ID}
    </select>

    <select id="findPWByUid" resultType="String" parameterType="int">
        select PW from user_info where Uid = #{Uid}
    </select>

    <select id="findUnameByUid" resultType="String" parameterType="int">
        select Uname from user_info where Uid = #{Uid}
    </select>

</mapper>